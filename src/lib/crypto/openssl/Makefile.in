mydir=lib$(S)crypto$(S)openssl
BUILDTOP=$(REL)..$(S)..$(S)..
SUBDIRS=enc_provider hash_provider
LOCALINCLUDES=-I$(srcdir)/../krb $(CRYPTO_IMPL_CFLAGS)

STLIBOBJS=\
	cmac.o	\
	hmac.o	\
	kdf.o	\
	pbkdf2.o \
	sha256.o

OBJS=\
	$(OUTPRE)cmac.$(OBJEXT)	\
	$(OUTPRE)hmac.$(OBJEXT)	\
	$(OUTPRE)kdf.$(OBJEXT)	\
	$(OUTPRE)pbkdf2.$(OBJEXT) \
	$(OUTPRE)sha256.$(OBJEXT)

SRCS=\
	$(srcdir)/cmac.c	\
	$(srcdir)/hmac.c	\
	$(srcdir)/kdf.c		\
	$(srcdir)/pbkdf2.c	\
	$(srcdir)/sha256.c

STOBJLISTS= md4/OBJS.ST 		\
	md5/OBJS.ST sha1/OBJS.ST sha2/OBJS.ST 	\
	enc_provider/OBJS.ST 		\
	hash_provider/OBJS.ST 		\
	aes/OBJS.ST 			\
	OBJS.ST

SUBDIROBJLISTS= md4/OBJS.ST 		\
		md5/OBJS.ST sha1/OBJS.ST sha2/OBJS.ST 	\
		enc_provider/OBJS.ST 		\
		hash_provider/OBJS.ST 		\
		aes/OBJS.ST 

all-unix: all-libobjs
includes: depend

depend: $(SRCS)

clean-unix:: clean-libobjsn

@lib_frag@
@libobj_frag@

